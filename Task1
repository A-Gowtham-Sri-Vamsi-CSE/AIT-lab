from collections import deque

graph = {
    'Google': ['Amazon', 'Microsoft'],
    'Amazon': ['Apple', 'IBM'],
    'Microsoft': ['Meta'],
    'Apple': [],
    'IBM': [],
    'Meta': ['Netflix'],
    'Netflix': []
}

def find_impacted(graph, start):
    visited = set()
    queue = deque([(start, 0)])
    result = []

    while queue:
        company, level = queue.popleft()

        if level == 2:
            continue

        for neighbor in graph.get(company, []):
            if neighbor not in visited:
                visited.add(neighbor)
                result.append(neighbor)
                queue.append((neighbor, level + 1))

    return result

start = 'Google'
impacted = find_impacted(graph, start)
print("Impacted companies:", impacted)
